{
  "functions": {
    "api/generate-story.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/upload-image.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "buildCommand": "npx vite build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
